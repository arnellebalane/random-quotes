{
  "name": "random-quotes",
  "version": "0.0.0-development",
  "description": "Get random quotes",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "nyc -x source/*.test.js mocha source/*.test.js --require babel-register",
    "test:watch": "npm test -- -w",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat coverage/lcov.info | codecov",
    "prebuild": "rm -rf dist",
    "build": "babel -d dist --ignore *.test.js source"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arnellebalane/random-quotes.git"
  },
  "keywords": [
    "quotes"
  ],
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arnellebalane/random-quotes/issues"
  },
  "homepage": "https://github.com/arnellebalane/random-quotes#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "nyc": "11.2.1",
    "semantic-release": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm test && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
