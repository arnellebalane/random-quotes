{
  "name": "random-quotes",
  "version": "0.0.0-development",
  "description": "Get random quotes",
  "main": "source/index.js",
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "istanbul cover -x source/*.test.js _mocha -- source/*.test.js",
    "test:watch": "npm test -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arnellebalane/random-quotes.git"
  },
  "keywords": [
    "quotes"
  ],
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arnellebalane/random-quotes/issues"
  },
  "homepage": "https://github.com/arnellebalane/random-quotes#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "4.0.1",
    "semantic-release": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm test"
    }
  }
}
